<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gudim.android.jogger" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SessionListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="org.gudim.android.jogger.SessionDetailActivity"
            android:label="@string/title_session_detail"
            android:parentActivityName="org.gudim.android.jogger.SessionListActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="org.gudim.android.jogger.SessionListActivity" />
        </activity>
    </application>
</manifest>

